{
  "name": "blitz-tcg",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "deploy:preview": "firebase hosting:channel:deploy preview_collections_page ",
    "deploy": "ng run blitz-tcg:prerender && firebase deploy --only hosting:blitz-tcg",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run blitz-tcg:serve-ssr",
    "serve:ssr": "node dist/blitz-tcg/server/main.js",
    "serve:prerender": "http-server -c-1 dist/blitz-tcg/browser",
    "build:ssr": "ng build && ng run blitz-tcg:server",
    "prerender": "ng run blitz-tcg:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.11",
    "@angular/common": "^13.3.11",
    "@angular/compiler": "^13.3.11",
    "@angular/core": "^13.3.11",
    "@angular/fire": "^7.4.1",
    "@angular/forms": "^13.3.11",
    "@angular/platform-browser": "^13.3.11",
    "@angular/platform-browser-dynamic": "^13.3.11",
    "@angular/platform-server": "^13.3.11",
    "@angular/router": "^13.3.11",
    "@nguniversal/express-engine": "^13.1.1",
    "@popperjs/core": "^2.11.8",
    "@types/file-saver": "^2.0.7",
    "angular-svg-icon": "^13.0.0",
    "axios": "^0.28.1",
    "bootstrap": "^5.2.2",
    "express": "^4.15.2",
    "file-saver": "^2.0.5",
    "firebase": "^9.9.0",
    "http-server": "^14.1.1",
    "jquery": "^3.6.0",
    "postcss": "~8.4.31",
    "rxjs": "~7.5.0",
    "semver": "~7.5.2",
    "sweetalert2": "^11.11.0",
    "swiper": "^8.1.4",
    "tar": "~6.2.1",
    "tslib": "^2.3.0",
    "webpack-dev-middleware": "~5.3.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.11",
    "@angular/cli": "^13.3.11",
    "@angular/compiler-cli": "^13.3.11",
    "@nguniversal/builders": "^13.1.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2",
    "webpack-dev-server": "^4.7.3"
  }
