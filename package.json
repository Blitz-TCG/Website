{
  "name": "blitz-tcg",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "deploy:preview": "firebase hosting:channel:deploy preview_collections_page ",
    "deploy": "ng run blitz-tcg:prerender && firebase deploy --only hosting:blitz-tcg",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run blitz-tcg:serve-ssr",
    "serve:ssr": "node dist/blitz-tcg/server/main.js",
    "serve:prerender": "http-server -c-1 dist/blitz-tcg/browser",
    "build:ssr": "ng build && ng run blitz-tcg:server",
    "prerender": "ng run blitz-tcg:prerender"
  },
  "private": true,
  "resolutions": {
    "webpack-dev-middleware": "5.3.4"
  },
  "dependencies": {
    "@angular/animations": "^19.2.14",
